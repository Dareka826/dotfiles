syntax on

colorscheme inkpot
set t_Co=256
let g:powerline_pycmd="py3"
:set laststatus=2

set encoding=utf-8
set number
set autoindent
set noerrorbells
set tabstop=4

no <Left> <Nop>
no <Down> <Nop>
no <Up> <Nop>
no <Right> <Nop>

ino <Left> <Nop>
ino <Down> <Nop>
ino <Up> <Nop>
ino <Right> <Nop>

vno <Left> <Nop>
vno <Down> <Nop>
vno <Up> <Nop>
vno <Right> <Nop>
